    <method jni-name="onPtrStatusChange" parameter="eventType" clr-enum-type="Epson.Xamarin.Android.Enums.Printer.Event" />
  </mapping>
  <mapping jni-class="com/epson/epos2/printer/ReceiveListener">
    <method jni-name="onPtrReceive" parameter="code" clr-enum-type="Epson.Xamarin.Android.Enums.Printer.Code" />
  </mapping>
